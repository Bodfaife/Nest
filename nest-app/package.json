{
  "name": "nest-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules --respawn src/main.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate",
    "migration:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "db:init": "node scripts/ensure-uuid.js",
    "webhook:smoke": "node scripts/webhook-smoke.js",
    "webhook:stripe-smoke": "node scripts/stripe-smoke.js"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/throttler": "^3.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.0",
    "sqlite3": "^5.1.6",
    "stripe": "^11.12.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "helmet": "^6.0.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.0.4",
    "@nestjs/cli": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/passport-jwt": "^3.0.9"
  }
}
